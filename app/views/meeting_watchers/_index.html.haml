%ul
  - @watchers.each do |user|
    %li{class: "user-#{user.id}"}
      = avatar(user, size: 16)
      = link_to_user(user, class: 'user')
      - if can_destroy_watcher?(@object)
        - url = {controller: 'meeting_watchers', action: 'destroy', id: user.id, meeting_container_id: @object.id, meeting_container_type: @object.class}
        = link_to(image_tag('delete.png'), url, remote: true, method: 'delete', class: "delete")
