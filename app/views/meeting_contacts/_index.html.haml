%ul
  - @contacts.each do |contact|
    %li{class: "contact-#{contact.id}"}
      = avatar_to(contact, size: 16)
      = link_to_contact(contact, class: 'contact')
      - url = {controller: 'meeting_contacts', action: 'destroy', id: contact.id, meeting_container_id: @object.id, meeting_container_type: @object.class}
      = link_to(image_tag('delete.png'), url, remote: true, method: 'delete', class: "delete")
